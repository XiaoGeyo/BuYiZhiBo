<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    tools:context="mzy.bc.com.buyizhibo.activity.HomeActivity">
    <RelativeLayout
        android:id="@+id/rl_home_frament"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/rl_home_bottom"
        >


    </RelativeLayout>

    <LinearLayout
        android:id="@+id/rl_home_bottom"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="#ffff"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imgbtn_home_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="#ffffff"
            android:src="@mipmap/tab_live_p"
             />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="65dp"
            android:layout_gravity="bottom"
            android:layout_weight="1"
            android:background="#00ffffff"
            android:src="@mipmap/tab_room" />

        <ImageView
            android:id="@+id/imgbtn_home_wode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="#ffff"
            android:src="@mipmap/tab_me" />
    </LinearLayout>
    <!--<RelativeLayout-->
        <!--android:id="@+id/rl_home_frament"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--android:layout_above="@+id/rl_home_bottom"-->
        <!--&gt;</RelativeLayout>-->


</RelativeLayout>
